FROM microsoft/dotnet:2.1-sdk-stretch-arm32v7 AS builder
    
ENV NUGET_XMLDOC_MODE skip
COPY . /src
RUN dotnet build -c Release -r linux-arm /src/HotFix.Benchmark/HotFix.Benchmark.csproj

ENTRYPOINT ["dotnet", "run", "-c", "Release", "-r", "linux-arm", "-p", "/src/HotFix.Benchmark/HotFix.Benchmark.csproj"]